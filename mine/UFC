from turtle import*
speed(20)
penup()
goto(-150,-300)
pendown()
width (3)
#start
forward (270)
left(45)
forward (270)
left(45)
forward (270)
left(45)
forward (270)
left(45)
forward (270)
left(45)
forward (270)
left(45)
forward (270)
left(45)
forward (270)
left(45)
#end of octagon



#starting UFC

#U
penup()
goto(-270,150)
pendown()
color("red")
width(20)
right(90)
forward(250)
left(90)
forward(130)
left(90)
forward(250)

#F
penup()
goto(-100,150)
pendown()
right(180)
forward(250) 

penup()
goto(-100,150)
pendown()
left(90)
forward(130)
penup()
goto(-100,40)
pendown()
forward(130)



























































exitonclick()

























































































